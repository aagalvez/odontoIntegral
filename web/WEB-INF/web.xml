<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>mySQLHost</param-name>
        <param-value>jdbc:mysql://localhost:4306/</param-value>
    </context-param>
    <context-param>
        <param-name>dbName</param-name>
        <param-value>odontologia</param-value>
    </context-param>
    <context-param>
        <param-name>user</param-name>
        <param-value>root</param-value>
    </context-param>
    <context-param>
        <param-name>password</param-name>
        <param-value>n0m3l0</param-value>
    </context-param>
    <servlet>
        <servlet-name>ServletCita</servlet-name>
        <servlet-class>servlets.ServletCita</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletControladorD</servlet-name>
        <servlet-class>servlets.ServletControladorD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletControladorC</servlet-name>
        <servlet-class>servlets.ServletControladorP</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletEquipo</servlet-name>
        <servlet-class>servlets.ServletEquipo</servlet-class>
        <init-param>
            <param-name>Name</param-name>
            <param-value>Value</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletCita</servlet-name>
        <url-pattern>/cita</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletControladorD</servlet-name>
        <url-pattern>/controladorD</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletControladorC</servlet-name>
        <url-pattern>/controladorC</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletEquipo</servlet-name>
        <url-pattern>/ServletEquipo</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
